terraform {
  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "~> 3.27"
    }
  }

  required_version = ">= 0.14.9"
}

provider "aws" {
  profile = "default"
  region  = "us-east-1"
}

resource "aws_instance" "app_server" {
  ami           = "ami-0b0dcb5067f052a63"
  subnet_id     = "subnet-015d6fae3fc198b65"
  instance_type = "t2.medium"

  tags = {
    Name = "bolingo"
  }
}
